<h1 class="title">Entry Index</h1>

<table>
  <thead> 
    <tr>
      <th>Title </th>
      <th>Content </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= entry.content %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
<br>
