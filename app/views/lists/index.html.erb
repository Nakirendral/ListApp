<h1 class="title">List Index</h1>

<table>
  <thead> 
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.list_name %></td>
        <td><%= link_to "Show", list %></td>
        <td><%= link_to "Edit", edit_list_path(list) %></td>
        <td><%= link_to "Delete", list, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
